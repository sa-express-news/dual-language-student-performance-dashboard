{
  "name": "dual-language-student-performance-dashboard",
  "version": "1.0.0",
  "description": "Analyze Bexar County student performance",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "serve": "webpack-dev-server --env dev --mode development",
    "build": "webpack --env prod --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sa-express-news/dual-language-student-performance-dashboard.git"
  },
  "keywords": [
    "bexar",
    "county",
    "dual",
    "language",
    "ell",
    "esl"
  ],
  "author": "Luke Whyte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sa-express-news/dual-language-student-performance-dashboard/issues"
  },
  "homepage": "https://github.com/sa-express-news/dual-language-student-performance-dashboard#readme",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
